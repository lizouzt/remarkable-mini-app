.m-header image{
  display: block;
  margin: 0 auto;
}

.mh-link{
  display: inline-block;
  float: right;
}
.mh-link mIcon{
  margin: 1px 0;
}
.mh-info{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  border-radius: 20rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  padding: 30rpx 5vw 10rpx;
}

.m-s-title .m-avatar{
  width: 10vw;
  height: 10vw;
  margin-right: 3vw;
}